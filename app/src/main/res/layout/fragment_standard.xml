<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
   android:gravity="center"
    tools:context="com.example.supratim.myapplication.standard">

    <!-- TODO: Update blank fragment layout -->


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:gravity="center"
        android:orientation="vertical"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/bt1"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="!"
                android:elevation="5dp"
                />
            <Button
                android:id="@+id/bt2"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="^"
                android:elevation="5dp"
                />
            <Button
                android:id="@+id/bt3"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="\u221A"
                android:elevation="5dp"
                />
            <Button
                android:id="@+id/bt4"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="\u03C0"
                />
            <Button
                android:id="@+id/bt5"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="C"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/bt6"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="sin"
                />
            <Button
                android:id="@+id/bt7"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="("
                />
            <Button
                android:id="@+id/bt8"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text=")"
                />
            <Button
                android:id="@+id/bt9"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="e"
                />
            <Button
                android:id="@+id/bt10"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="DEL"
                />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="horizontal">

            <Button
                android:id="@+id/bt11"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="cos"
                android:elevation="5dp"
                />
            <Button
                android:id="@+id/bt12"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="7"
                android:elevation="5dp"
                />
            <Button
                android:id="@+id/bt13"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="8"
                android:elevation="5dp"
                />
            <Button
                android:id="@+id/bt14"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:text="9"
                android:elevation="5dp"
                />
            <Button
                android:id="@+id/bt15"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="/"
                />

        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/bt16"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="tan"
                />
            <Button
                android:id="@+id/bt17"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="4"
                />
            <Button
                android:id="@+id/bt18"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="5"
                />
            <Button
                android:id="@+id/bt19"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="6"
                />
            <Button
                android:id="@+id/bt20"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="*"
                />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/bt21"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="ln"
                />
            <Button
                android:id="@+id/bt22"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="1"
                />
            <Button
                android:id="@+id/bt23"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="2"

                />
            <Button
                android:id="@+id/bt24"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="3"
                />
            <Button
                android:id="@+id/bt25"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="-"
                />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <Button
                android:id="@+id/bt26"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="log"
                />
            <Button
                android:id="@+id/bt27"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="0"
                />
            <Button
                android:id="@+id/bt28"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="."

                />
            <Button
                android:id="@+id/bt29"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="="
                />
            <Button
                android:id="@+id/bt30"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:elevation="5dp"
                android:text="+"
                />


        </LinearLayout>

    </LinearLayout>

</FrameLayout>
